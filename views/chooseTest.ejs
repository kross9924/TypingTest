<%-include("./partials/header.ejs") %>
<br>
<div class="container">
    <h2>Select a test</h2>
    <%tests.forEach(function(test){%>
        <a href="/taketest/<%=test._id%>">
            <div class="row">
                <div class="col-sm-12">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title"><%=test.name%></h5>
                            <p class="card-text"><%for(var i = 0;i<200;i++){%><%=test.data[i]%><%}%>...</p>
                        </div>
                    </div>
                </div>
            </div>
            <br>
        </a>
        <%});%>
</div>

<%-include("./partials/footer.ejs") %>
