<%- include("./partials/header.ejs") %>


<div class="container">
  <br>
  <br>
  <div class="row">
    <div class="col-sm-0 col-md-2 col-lg-3 col-xl-3">
  
    </div>
    <div class="col-sm-12 col-md-8 col-lg-6 col-xl-6">
      <div class="card text-center ">
        <div class="card-header">
          <div>Time Left:<span id="timeLeftMin">5</span>:<span id="timeLeftSec">00</span></div>
          <div>Typing Speed:<span id="typingSpeed">0</span></div>
        </div>
        <div class="card-body scrollbar ex" >
          <%var str = test.data;str = str.replace(/\n/g," ");str = str.replace(/\s\s+/g, ' ');;var Data = str.split(" ");%>
          <%Data.forEach(function(word){%>
                <span class="wordsSpan"><%=word%></span>
          <%})%>
        </div>
        
      </div>
      
      <input type="text" id="typing">
    
    </div>
    <div class="col-sm-0 col-md-2 col-lg-3 col-xl-3">
      
    </div>
  </div>

</div>
    
    
<script type="text/javascript" src="/js/typingtest.js"></script>
